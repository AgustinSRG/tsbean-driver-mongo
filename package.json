{
  "name": "tsbean-driver-mongo",
  "version": "2.1.1",
  "description": "Mongo DB driver for TSBean-ORM.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/AgustinSRG/tsbean-driver-mongo",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgustinSRG/tsbean-driver-mongo"
  },
  "scripts": {
    "prebuild": "npm run lint",
    "compile": "tsc --pretty --declaration",
    "build": "rimraf dist && npm run compile && npm run gendoc",
    "lint": "eslint --fix src/**/*.ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "gendoc": "typedoc --out docs ./src"
  },
  "files": [
    "dist/"
  ],
  "author": {
    "name": "Agustin San Roman",
    "email": "agustinsanromanguzman@gmail.com",
    "url": "https://github.com/AgustinSRG"
  },
  "engines": {
    "node": ">= 12.9.0"
  },
  "pre-commit": [
    "lint"
  ],
  "peerDependencies": {
    "tsbean-orm": "^1.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "mocha": "^9.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "license": "MIT",
  "dependencies": {
    "mongodb": "^4.4.0",
    "tsbean-orm": "^1.1.1"
  }
}
